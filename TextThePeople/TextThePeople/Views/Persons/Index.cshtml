@model IEnumerable<TextThePeople.Models.Persons>

@{
    ViewBag.Title = "Index";
}

<h2>Clients</h2>

<table>
    <tr>
        <th>
            Person ID Number
        </th>
        <th>
            Phone Number
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OSEntityPK)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PersonsId })
        </td>
    </tr>
}

</table>
